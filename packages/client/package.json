{
  "name": "fluxprotocol-client",
  "version": "0.0.2",
  "description": "FLUX client library for HTTP and WebSocket",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/ws": "^8.5.13",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "rimraf": "^6.0.1"
  },
  "keywords": ["flux", "client", "mcp", "ai"],
  "license": "MIT"
}
