{
  "name": "fluxprotocol-client",
  "version": "0.0.6",
  "description": "FLUX client library for HTTP and WebSocket",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
